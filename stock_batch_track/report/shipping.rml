  <story>
    <pto>
    <pto_header>
    <blockTable colWidths="350.0,127.0,200.0,87.0" repeatRows="1" style="Table3">
      <tr>
        <td>
          <para style="P7">[Code] Description</para>
        </td>
        <td>
          <para style="P6">Qty.</para>
        </td>
        <td>
          <para style="P5">Source</para>
        </td>
        <td>
          <para style="P5">Lot</para>
        </td>
      </tr>
    </blockTable>
    </pto_header>
    <para style="terp_default_9">[[repeatIn(objects,'o')]]</para>
    <para style="P15">Delivery Order : [[ o.name ]]</para>
    <blockTable colWidths="380.0,3.0,380.0" repeatRows="1" style="Tableau1">
      <tr>
        <td>
          <para style="P10">Subcontractor: [[ o.partner_id and o.partner_id.name or '' ]]</para>
          <para style="P10">Address: [[ o.partner_id.address and o.partner_id.address[0].location.name or '']]</para>
          <para style="P10">Tel: [[ o.partner_id.address and o.partner_id.address[0].phone or removeParentNode('para')]]</para>
          <para style="P10">Contactor: [[ o.partner_id.address and o.partner_id.address[0].name or removeParentNode('para') ]] </para>
        </td>
        <td>
        </td>
        <td>
          <para style="P10">Reference: [[ o.serial  or '' ]]</para>
          <para style="P10">Pack Date: [[ o.date[:10] or '']]</para>
          <para style="P10">Print Date: [[ time.strftime('%Y-%m-%d') ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="P3">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="350.0,127.0,200.0,87.0" repeatRows="1" style="Table3">
      <tr>
        <td>
          <para style="P7">[Code] Description</para>
        </td>
        <td>
          <para style="P6">Qty.</para>
        </td>
        <td>
          <para style="P5">Source</para>
        </td>
        <td>
          <para style="P5">Lot</para>
        </td>
      </tr>
    </blockTable>
    <section>
      <para style="P16">[[ repeatIn([line for line in o.move_ids if (line.product_qty >0 and (line.state == 'confirmed' or line.state=='done' or line.state=='assigned'))],'line') ]]</para>
      <blockTable colWidths="349.0,127.0,200.0,87.0" style="Table4">
        <tr>
          <td>
            <para style="terp_default_9"><font face="Helvetica" size="9.0">[ [[ (line.product_id.default_code) or removeParentNode('font') ]] ]</font> [[ line.product_id.name ]] [[ line.product_id.variants or '']]</para>
            <para style="terp_default_9">[[line.partner_id and get_partner_ref(line.partner_id.id, line.product_id.id) or removeParentNode('para')]]</para>
          </td>
          <td>
            <para style="terp_default_Right_9">[[ formatLang(line.product_qty, digits=get_digits(dp='Product UoM')) ]] [[ line.product_uom and line.product_uom.name ]]</para>
          </td>
          <td>
            <para style="terp_default_9">[[ line.origin or line.picking_id and line.picking_id.name or '' ]]</para>
          </td>
          <td>
            <para style="terp_default_9">[[ (line.prodlot_id and (line.prodlot_id.name + (line.prodlot_id.ref and ('/' + line.prodlot_id.ref) or ''))) or ' ' ]]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P11">
        <font color="white"> </font>
      </para>
    </section>
